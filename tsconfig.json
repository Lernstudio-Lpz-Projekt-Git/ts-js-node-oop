{
  "compilerOptions": {
    "lib": ["dom","es2021"],
    "module": "commonjs",
    "target": "es2021",
    "watch": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "./build",
    "node_moduls"
  ]

}